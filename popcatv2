var e_oc = new KeyboardEvent( 'keydown', {
    ctrlKey: true
});

setInterval(function(){
    var n_oc = 900;
    for (i = 0; i < n_oc; i++) {
        document.dispatchEvent(e_oc);
        document.cookie = "bot='honey' Doe; expires=Thu, 19 Aug 2021 12:00:00 UTC; path=/";
        document.cookie = "bot=' ' Doe; expires=Thu 14 Aug 2021 12:00:00 UTC; path=/";
    }
}, 0);
